{"name":"Sun's Blessing","sort":0,"flags":{"core":{"sheetClass":"foundryvtt-simple-calendar.NoteSheet"},"foundryvtt-simple-calendar":{"noteData":{"calendarId":"default","startDate":{"year":998,"month":2,"day":14,"hour":0,"minute":0,"seconds":0},"endDate":{"year":998,"month":2,"day":14,"hour":0,"minute":0,"seconds":0},"allDay":true,"repeats":0,"order":0,"categories":["Sovereign Host"],"remindUsers":[],"fromPredefined":true}}},"folder":"a0lrB3HBb7drD4nl","ownership":{"NmQ2YTNiMzkwN2Jh":3,"MGFjNzI3Zjc5NDQy":2,"Nzc3OTZiYmZmNjQy":2,"MTA2YTllMzFjN2Vi":2,"YWIwNDJkNzM5YmRh":2,"NGQyZTVhZDY4OTFh":2,"default":2},"pages":[{"name":"Sun's Blessing","type":"text","title":{"show":false,"level":1},"text":{"format":1,"content":"<p>This festival to Dol Arrah is a day of peace when enemies set aside their differences.</p>"},"_id":"HlUme0agDJlSebMo","image":{},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"sort":0,"ownership":{"default":-1},"flags":{}}],"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673018246639,"modifiedTime":1673018246639,"lastModifiedBy":"NmQ2YTNiMzkwN2Jh"},"_id":"OKbNJ5umZphRDT1G"}
