derdark":1,"UAWaterborneAdventures":1,"OotDLPG":1,"UAFighter":1,"EGW":1,"UAGothicHeroes":1,"UA2020PsionicOptionsRevisited":1,"UAFighterRogueWizard":1,"UAFighterRangerRogue":1,"FTD":1,"UA2020SubclassesPt5":1,"UAATrioOfSubclasses":1,"UAMonk":1,"UATheMysticClass":1,"UAPaladin":1,"UAPrestigeClassesRunMagic":1,"UARangerAndRogue":1,"UARanger":1,"UATheRangerRevised":1,"UAModifyingClasses":1,"UA2020SubclassesRevisited":1,"UASorcererAndWarlock":1,"UASorcerer":1,"UAGiantSoulSorcerer":1,"UA2022HeroesOfKrynn":1,"DSotDQ":1,"PSK":1,"UAWarlockAndWizard":1,"UATheFaithful":1,"UAEberron":1,"UAWizardRevisited":1,"_isActive":true,"_totals":{"yes":63,"no":0,"ignored":0},"_combineBlue":"or","_combineRed":"or"},"Miscellaneous":{"Reprinted":2,"Sidekick":2,"SRD":0,"Basic Rules":0,"_isActive":true,"_totals":{"yes":0,"no":2,"ignored":2},"_combineBlue":"or","_combineRed":"or"},"Other/Text Options":{"isDisplayClassIfSubclassActive":false,"isClassFeatureVariant":true,"_isActive":false}},"isPrimaryClass":true,"isDirectImport":true}},"effects":[],"img":"https://assets.forge-vtt.com/bazaar/core/icons/magic/earth/projectiles-magma-stone-orange.webp","folder":null,"sort":0,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473556875,"modifiedTime":1673473591468,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"_id":"UHNjU4UTAGK9Luzq","name":"Shadow Magic","type":"subclass","system":{"description":{"value":"<div class=\"mb-2 ve-flex-col\"><div class=\"rd__b  rd__b--1\"><p>You are a creature of shadow, for your innate magic comes from the Shadowfell itself. You might trace your lineage to an entity from that place, or perhaps you were exposed to its fell energy and transformed by it.</p><p>The power of shadow magic casts a strange pall over your physical presence. The spark of life that sustains you is muffled, as if it struggles to remain viable against the dark energy that imbues your soul. At your option, you can pick from or roll on the Shadow Sorcerer Quirks table to create a quirk for your character.</p><table class=\"w-100 rd__table  stripe-odd-table\"><caption>Shadow Sorcerer Quirks</caption><thead><tr><th class=\"col-2 text-center\" data-rd-isroller=\"true\">[[/r d6]]</th><th class=\"col-10\" data-rd-isroller=\"false\">Quirk</th></tr></thead><tbody><tr><td class=\"col-2 text-center\" data-roll-min=\"1\" data-roll-max=\"1\">1</td><td class=\"col-10\">You are always icy cold to the touch.</td></tr><tr><td class=\"col-2 text-center\" data-roll-min=\"2\" data-roll-max=\"2\">2</td><td class=\"col-10\">When you are asleep, you don't appear to breathe (though you must still breathe to survive).</td></tr><tr><td class=\"col-2 text-center\" data-roll-min=\"3\" data-roll-max=\"3\">3</td><td class=\"col-10\">You barely bleed, even when badly injured.</td></tr><tr><td class=\"col-2 text-center\" data-roll-min=\"4\" data-roll-max=\"4\">4</td><td class=\"col-10\">Your heart beats once per minute. This event sometimes surprises you.</td></tr><tr><td class=\"col-2 text-center\" data-roll-min=\"5\" data-roll-max=\"5\">5</td><td class=\"col-10\">You have trouble remembering that living creatures and corpses should be treated differently.</td></tr><tr><td class=\"col-2 text-center\" data-roll-min=\"6\" data-roll-max=\"6\">6</td><td class=\"col-10\">You blinked. Once. Last week.</td></tr></tbody></table></div></div>","chat":"","unidentified":""},"source":"XGE","identifier":"shadow-magic","classIdentifier":"sorcerer","advancement":[{"_id":"U3Nw3vSxsAZzm5yq","type":"ItemGrant","level":1,"title":"Features","icon":"https://assets.forge-vtt.com/bazaar/core/icons/magic/earth/projectiles-magma-stone-orange.webp","configuration":{"items":[]},"value":{"added":{"gxHZE5jWa7s3GQGG":"","6Q1UJvoWB0ZExY9N":""}}},{"_id":"9x5IeB03hdHt0L63","type":"ItemGrant","level":6,"title":"Features","icon":"https://assets.forge-vtt.com/bazaar/core/icons/magic/earth/projectiles-magma-stone-orange.webp","configuration":{"items":[]},"value":{"added":{"4r5NtryKtuAkPRSu":""}}}],"spellcasting":{"progression":"none","ability":""}},"flags":{"plutonium":{"page":"classes.html","source":"XGE","hash":"sorcerer_phb,state:sub-shadow-xge=b1","propDroppable":"subclass","filterValues":{"Source":{"TCE":1,"ERLW":1,"UA2020SubclassesPt3":1,"UAArtificerRevisited":1,"UAClassFeatureVariants":1,"UAArtificer":1,"PHB":1,"XGE":1,"UABarbarianPrimalPaths":1,"UARevisedSubclasses":1,"SCAG":1,"UA2020SubclassesPt1":1,"UA2022GiantOptions":1,"UABarbarianAndMonk":1,"UA2020SubclassesPt2":1,"UABardAndPaladin":1,"UABardBardColleges":1,"UAKitsOfOld":1,"VRGR":1,"UA2020SubclassesPt4":1,"UA2021MagesOfStrixhaven":1,"PSA":1,"UAModernMagic":1,"DMG":1,"UA2022WondersOfTheMultiverse":1,"UAClericDivineDomains":1,"UAOrderDomain":1,"UAClericDruidWizard":1,"UADruid":1,"UAThreeSubclasses":1,"UARevisedClassOptions":1,"UASidekicks":1,"UALightDarkUnderdark":1,"UAWaterborneAdventures":1,"OotDLPG":1,"UAFighter":1,"EGW":1,"UAGothicHeroes":1,"UA2020PsionicOptionsRevisited":1,"UAFighterRogueWizard":1,"UAFighterRangerRogue":1,"FTD":1,"UA2020SubclassesPt5":1,"UAATrioOfSubclasses":1,"UAMonk":1,"UATheMysticClass":1,"UAPaladin":1,"UAPrestigeClassesRunMagic":1,"UARangerAndRogue":1,"UARanger":1,"UATheRangerRevised":1,"UAModifyingClasses":1,"UA2020SubclassesRevisited":1,"UASorcererAndWarlock":1,"UASorcerer":1,"UAGiantSoulSorcerer":1,"UA2022HeroesOfKrynn":1,"DSotDQ":1,"PSK":1,"UAWarlockAndWizard":1,"UATheFaithful":1,"UAEberron":1,"UAWizardRevisited":1,"_isActive":true,"_totals":{"yes":63,"no":0,"ignored":0},"_combineBlue":"or","_combineRed":"or"},"Miscellaneous":{"Reprinted":2,"Sidekick":2,"SRD":0,"Basic Rules":0,"_isActive":true,"_totals":{"yes":0,"no":2,"ignored":2},"_combineBlue":"or","_combineRed":"or"},"Other/Text Options":{"isDisplayClassIfSubclassActive":false,"isClassFeatureVariant":true,"_isActive":false}},"isPrimaryClass":true,"isDirectImport":true}},"effects":[],"img":"https://assets.forge-vtt.com/bazaar/core/icons/magic/earth/projectiles-magma-stone-orange.webp","folder":null,"sort":0,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473557083,"modifiedTime":1673473591668,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"name":"Spellcasting","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--3\"><p>An event in your past, or in the life of a parent or ancestor, left an indelible mark on you, infusing you with arcane magic. This font of magic, whatever its origin, fuels your spells. See <a href=\"https://5etools-mirror-1.github.io/book.html#PHB,10\">chapter 10</a> for the general rules of spellcasting and <a href=\"https://5etools-mirror-1.github.io/book.html#PHB,11\">chapter 11</a> for the <a href=\"https://5etools-mirror-1.github.io/spells.html#blankhash,flstclass:sorcerer=1\">sorcerer spell list</a>.</p><div class=\"rd__spc-inline-post\"></div><div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"31\"> <span class=\"entry-title-inner\">Cantrips.</span></span> <p>At 1st level, you know four cantrips of your choice from the sorcerer spell list. You learn an additional sorcerer cantrip of your choice at 4th level and another at 10th level.</p><div class=\"rd__spc-inline-post\"></div></div><div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"32\"> <span class=\"entry-title-inner\">Spell Slots.</span></span> <p>The Sorcerer table shows how many spell slots you have to cast your <a href=\"https://5etools-mirror-1.github.io/spells.html#blankhash,flstclass:sorcerer=1\">sorcerer spells</a> of 1st level and higher. To cast one of these sorcerer spells, you must expend a slot of the spell's level or higher. You regain all expended spell slots when you finish a long rest.</p><div class=\"rd__spc-inline-post\"></div><p>For example, if you know the 1st-level spell @spell[burning hands] and have a 1st-level and a 2nd-level spell slot available, you can cast @spell[burning hands] using either slot.</p></div><div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"33\"> <span class=\"entry-title-inner\">Spells Known of 1st Level and Higher.</span></span> <p>You know two 1st-level spells of your choice from the sorcerer spell list.</p><div class=\"rd__spc-inline-post\"></div><p>You learn an additional sorcerer spell of your choice at each level except 12th, 14th, 16th, 18th, 19th, and 20th. Each of these spells must be of a level for which you have spell slots. For instance, when you reach 3rd level in this class, you can learn one new spell of 1st or 2nd level.</p><p>Additionally, when you gain a level in this class, you can choose one of the sorcerer spells you know and replace it with another spell from the sorcerer spell list, which also must be of a level for which you have spell slots.</p></div><div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"34\"> <span class=\"entry-title-inner\">Spellcasting Ability.</span></span> <p>Charisma is your spellcasting ability for your sorcerer spells, since the power of your magic relies on your ability to project your will into the world. You use your Charisma whenever a spell refers to your spellcasting ability. In addition, you use your Charisma modifier when setting the saving throw DC for a sorcerer spell you cast and when making an attack roll with one.</p><div class=\"rd__spc-inline-post\"></div><p></p><div class=\"text-center\"><b>Spell save DC</b> = 8 + your proficiency bonus + your Charisma modifier</div><p></p><p></p><div class=\"text-center\"><b>Spell attack modifier</b> = your proficiency bonus + your Charisma modifier</div><p></p></div><div class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"35\"> <span class=\"entry-title-inner\">Spellcasting Focus.</span></span> <p>You can use an @item[arcane focus|phb] as a spellcasting focus for your sorcerer spells.</p><div class=\"rd__spc-inline-post\"></div></div></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 1","recharge":{"value":null,"charged":false}},"effects":[],"flags":{"plutonium":{"page":"classFeature","source":"PHB","hash":"spellcasting_sorcerer_phb_1_phb"},"dnd5e":{"advancementOrigin":"ro2wccRiqQvgpDRG.vjZAZsQe6JdWxd49"}},"img":"https://assets.forge-vtt.com/bazaar/core/icons/magic/unholy/strike-hand-glow-pink.webp","_id":"JJzCFWuscdLibPSx","folder":null,"sort":800000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473557296,"modifiedTime":1673474305136,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"_id":"gxHZE5jWa7s3GQGG","name":"Eyes of the Dark","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p>Starting at 1st level, you have @sense[darkvision] with a range of 120 feet.</p><p>When you reach 3rd level in this class, you learn the @spell[darkness] spell, which doesn't count against your number of sorcerer spells known. In addition, you can cast it by spending 2 sorcery points or by expending a spell slot. If you cast it with sorcery points, you can see through the @spell[darkness] created by the spell.</p></div>","chat":"","unidentified":""},"source":"XGE","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 1 (Shadow)","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mighty-force.svg","flags":{"plutonium":{"page":"subclassFeature","source":"XGE","hash":"eyes%20of%20the%20dark_sorcerer_phb_shadow_xge_1_xge"},"dnd5e":{"advancementOrigin":"UHNjU4UTAGK9Luzq.U3Nw3vSxsAZzm5yq"},"favtab":{"isFavorite":true}},"effects":[],"folder":null,"sort":-50000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473557547,"modifiedTime":1673585947653,"lastModifiedBy":"NGQyZTVhZDY4OTFh"}},{"_id":"6Q1UJvoWB0ZExY9N","name":"Strength of the Grave","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p>Starting at 1st level, your existence in a twilight state between life and death makes you difficult to defeat. When damage reduces you to 0 hit points, you can make a Charisma saving throw (DC 5 + the damage taken). On a success, you instead drop to 1 hit point. You can't use this feature if you are reduced to 0 hit points by radiant damage or by a critical hit.</p><p>After the saving throw succeeds, you can't use this feature again until you finish a long rest.</p></div>","chat":"","unidentified":""},"source":"XGE","activation":{"type":"special","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":"1","per":"lr","recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["5",""]],"versatile":""},"formula":"","save":{"ability":"cha","dc":null,"scaling":"spell"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 1 (Shadow)","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mighty-force.svg","flags":{"plutonium":{"page":"subclassFeature","source":"XGE","hash":"strength%20of%20the%20grave_sorcerer_phb_shadow_xge_1_xge"},"dnd5e":{"advancementOrigin":"UHNjU4UTAGK9Luzq.U3Nw3vSxsAZzm5yq"},"favtab":{"isFavorite":true}},"effects":[],"folder":null,"sort":-25000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473557708,"modifiedTime":1673585954734,"lastModifiedBy":"NGQyZTVhZDY4OTFh"}},{"_id":"P13Bv7zTU0bg8bh2","name":"Spell Versatility","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p><i>1st-level sorcerer feature (enhances Spellcasting)</i></p><p>Whenever you finish a long rest, you can replace one spell you learned from this Spellcasting feature with another spell from the sorcerer spell list. The new spell must be the same level as the spell you replace.</p></div>","chat":"","unidentified":""},"source":"UACFV","activation":{"type":"special","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":1,"max":"1","per":"lr","recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 1","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mighty-force.svg","flags":{"plutonium":{"page":"classFeature","source":"UAClassFeatureVariants","hash":"spell%20versatility_sorcerer_phb_1_uaclassfeaturevariants"},"dnd5e":{"advancementOrigin":"ro2wccRiqQvgpDRG.vjZAZsQe6JdWxd49"}},"effects":[],"folder":null,"sort":200000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473557883,"modifiedTime":1673473670097,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"name":"Font of Magic","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--3\"><p>At 2nd level, you tap into a deep wellspring of magic within yourself. This wellspring is represented by sorcery points, which allow you to create a variety of magical effects.</p><div class=\"rd__spc-inline-post\"></div><p>@UUID[Actor.3Y3g2KKvnFnrD7Lz.Item.hl9VH7PTkG32ajLd]{Sorcery Points}</p><p>@UUID[Actor.3Y3g2KKvnFnrD7Lz.Item.GOWTBYeh8BMk6CLX]{Flexible Casting}</p></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":null,"cost":null,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":null,"per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 2","recharge":{"value":null,"charged":false}},"effects":[],"flags":{"plutonium":{"page":"classFeature","source":"PHB","hash":"font%20of%20magic_sorcerer_phb_2_phb"},"dnd5e":{"advancementOrigin":"ro2wccRiqQvgpDRG.v31QOBJXrWMl6TfF"}},"img":"https://assets.forge-vtt.com/bazaar/core/icons/magic/control/silhouette-hold-change-blue.webp","_id":"mz8Nqwx5CgRGypwm","folder":null,"sort":1000000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473558024,"modifiedTime":1673474305137,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"_id":"hl9VH7PTkG32ajLd","name":"Sorcery Points","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p>You have 2 sorcery points, and you gain one additional point every time you level up, to a maximum of 20 at level 20. You can never have more sorcery points than shown on the table for your level. You regain all spent sorcery points when you finish a long rest.</p></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"special","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":13,"max":13,"per":"lr","recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 2","recharge":{"value":null,"charged":false}},"img":"icons/magic/control/silhouette-hold-change-blue.webp","flags":{"plutonium":{"page":"classFeature","source":"PHB","hash":"sorcery%20points_sorcerer_phb_2_phb"},"dnd5e":{"advancementOrigin":"ro2wccRiqQvgpDRG.v31QOBJXrWMl6TfF"},"favtab":{"isFavorite":false}},"effects":[],"folder":null,"sort":-100000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473558128,"modifiedTime":1673585690181,"lastModifiedBy":"NGQyZTVhZDY4OTFh"}},{"_id":"GOWTBYeh8BMk6CLX","name":"Flexible Casting","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p>You can use your sorcery points to gain additional spell slots, or sacrifice spell slots to gain additional sorcery points. You learn other ways to use your sorcery points as you reach higher levels.</p><div class=\"rd__b  rd__b--2\"><h3 class=\"rd__h rd__h--2\" data-title-index=\"36\"> <span class=\"entry-title-inner\">Creating Spell Slots</span><span class=\"ve-flex-vh-center\"><span class=\"rd__h-toggle ml-2 clickable\" data-rd-h-toggle-button=\"true\">[–]</span></span></h3> <p>You can transform unexpended sorcery points into one spell slot as a bonus action on your turn. The created spell slots vanish at the end of a long rest. The Creating Spell Slots table shows the cost of creating a spell slot of a given level. You can create spell slots no higher in level than 5th.</p><table class=\"w-100 rd__table  stripe-odd-table\"><caption>Creating Spell Slots</caption><thead><tr><th class=\"col-6 text-center\" data-rd-isroller=\"false\">Spell Slot Level</th><th class=\"col-6 text-center\" data-rd-isroller=\"false\">Sorcery Point Cost</th></tr></thead><tbody><tr><td class=\"col-6 text-center\">1st</td><td class=\"col-6 text-center\">2</td></tr><tr><td class=\"col-6 text-center\">2nd</td><td class=\"col-6 text-center\">3</td></tr><tr><td class=\"col-6 text-center\">3rd</td><td class=\"col-6 text-center\">5</td></tr><tr><td class=\"col-6 text-center\">4th</td><td class=\"col-6 text-center\">6</td></tr><tr><td class=\"col-6 text-center\">5th</td><td class=\"col-6 text-center\">7</td></tr></tbody></table></div><div class=\"rd__b  rd__b--2\"><h3 class=\"rd__h rd__h--2\" data-title-index=\"37\"> <span class=\"entry-title-inner\">Converting a Spell Slot to Sorcery Points</span><span class=\"ve-flex-vh-center\"><span class=\"rd__h-toggle ml-2 clickable\" data-rd-h-toggle-button=\"true\">[–]</span></span></h3> <p>As a bonus action on your turn, you can expend one spell slot and gain a number of sorcery points equal to the slot's level.</p></div></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"type":{"value":"","subtype":""},"requirements":"Sorcerer 2","recharge":{"value":null,"charged":false}},"img":"modules/plutonium/media/icon/mighty-force.svg","flags":{"plutonium":{"page":"classFeature","source":"PHB","hash":"flexible%20casting_sorcerer_phb_2_phb"},"dnd5e":{"advancementOrigin":"ro2wccRiqQvgpDRG.v31QOBJXrWMl6TfF"}},"effects":[],"folder":null,"sort":400000,"ownership":{"default":0,"MTA2YTllMzFjN2Vi":3},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1673473558237,"modifiedTime":1673473670097,"lastModifiedBy":"MTA2YTllMzFjN2Vi"}},{"_id":"SuN3KrD0eHQZrXhf","name":"Font of Magic Options","type":"feat","system":{"description":{"value":"<div class=\"rd__b  rd__b--1\"><p><i>2nd-level sorcerer feature (enhances Font of Magic)</i></p><p>When you gain the Font of Magic feature, you get access to the following ways to spend your sorcery points.</p><p>@UUID[Actor.3Y3g2KKvnFnrD7Lz.Item.LJGwn0eCej0Bw2m9]{Empowering Reserves}</p><p>@UUID[Actor.3Y3g2KKvnFnrD7Lz.Item.cyWnWV8k3h6lg5hV]{Imbuing Touch}</p><p>@UUID[Actor.3Y3g2KKvnFnrD7Lz.Item.QrcPoij3wRtNxrfS]{Sorcerous Fortitude}</p></div>","chat":"","unidentified":""},"source":"UACFV","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"other","attackBonus":